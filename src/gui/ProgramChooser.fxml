<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ProgramChooserController">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Select a Program to Run">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Text>
        <ListView fx:id="programsListView" prefHeight="400.0" prefWidth="400.0" />
        <Button mnemonicParsing="false" onAction="#displayProgram" text="Start Program" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>